    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "shiki": "^3.20.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0"
  },
  "comments": {
    "gray-matter": "Parse frontmatter from markdown files",
    "remark": "Markdown processor with plugin ecosystem",
    "remark-html": "Convert markdown to HTML",
    "rehype-stringify": "HTML stringifier for rehype",
